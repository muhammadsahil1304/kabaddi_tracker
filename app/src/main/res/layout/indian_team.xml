<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_weight="3"
    android:orientation="vertical"
    android:background="@color/shailcolor">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:weightSum="3"
        android:orientation="horizontal">



        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="240dp"
                android:layout_margin="8dp"
                app:cardCornerRadius="10dp"
                android:elevation="8dp"
                app:cardBackgroundColor="@color/shailcolor1"
                android:layout_weight="1">

                <LinearLayout
                    android:id="@+id/rohithid"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/rohitimageid"
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                      
                        android:scaleType="centerCrop"
                        android:src="@drawable/rohitsharmaa">

                    </ImageView>

                    <TextView
                        android:id="@+id/rohithtextid"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Rohit Sharma"
                        android:textColor="@color/white"
                        android:gravity="center_horizontal"
                        android:padding="10dp"
                        android:textSize="16dp"></TextView>

                </LinearLayout>

            </androidx.cardview.widget.CardView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="240dp"
                android:layout_margin="8dp"
                app:cardCornerRadius="10dp"
                android:elevation="8dp"
                app:cardBackgroundColor="@color/shailcolor1"
                android:layout_weight="1">

                <LinearLayout
                    android:id="@+id/shubhanid"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/shubmam">

                    </ImageView>

                    <TextView
                        android:id="@+id/shubhantextid"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Shubhan gill"
                        android:textColor="@color/white"
                        android:gravity="center_horizontal"
                        android:padding="10dp"
                        android:textSize="16dp"></TextView>

                </LinearLayout>

            </androidx.cardview.widget.CardView>



        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="240dp"
                android:layout_margin="8dp"
                app:cardCornerRadius="10dp"
                android:elevation="8dp"
                app:cardBackgroundColor="@color/shailcolor1"
                android:layout_weight="1">

                <LinearLayout
                    android:id="@+id/viratid"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/viratt">

                    </ImageView>

                    <TextView
                        android:id="@+id/virattextid"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Virat Kohli"
                        android:textColor="@color/white"
                        android:gravity="center_horizontal"
                        android:padding="10dp"
                        android:textSize="16dp"></TextView>

                </LinearLayout>

            </androidx.cardview.widget.CardView>



        </LinearLayout>



    </LinearLayout>
    <LinearLayout
